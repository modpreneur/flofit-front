<script type="text/javascript">
    function typeUA() {
        var UA = navigator.userAgent;
        if (UA.match(/iPhone|iPod/i) !== null || (UA.match(/iPad/) && this.options.iOSUniversalApp)) {
            return 'ios';
        } else if (UA.match(/Android/i) !== null) {
            return 'android';
        }
    }
    var label = 'android';
    if (typeUA() == 'ios') {
        label = 'ios';
    } else if (typeUA() == 'android') {
        label = 'android';
    } else {
        label = 'desktop';
    }
</script>

