<script>
    var useTimers = true;
    $(document).ready(function() {
        $("img.lazy-notrigger").unveil(300);
    });

    // load all images 5s before first timer
    setTimeout(function() {
        if(useTimers) {
            console.log("USING timer 1");
            $("img.lazy-notrigger").trigger("unveil");
        }
    }, {{ timer1 - 5000 }});

    setTimeout(function(){
        if(useTimers){
            $("#hidden1").slideDown(500);
            var windowWidth = $(window).width();
            if (windowWidth <= 767) {
                $(".video").css("margin-bottom", "10px");
            } else {
                $(".video").css("margin-bottom", "20px");
            }

            setTimeout(function() {
                $("#clickbank-disclaimer").slideDown(500);
            }, 1000);
        }
    },{{ timer1 }});

    {% if isMobile %}
    // seeking event - if user seeek after 1st timer - show all content
    $(document).ready(function(){
        var searchInterval = setInterval(function(){
            $(document).find("video").each(function(i,item){
                if(item.id.search("wistia") != -1)
                {
                    item.onseeking = function() {
                        if(item.currentTime * 1000 >= {{ timer1 }})
                        {
                            useTimers = false;

                            $("#hidden1").slideDown(500);
                            var windowWidth = $(window).width();
                            if (windowWidth <= 767) {
                                $(".video").css("margin-bottom", "10px");
                            } else {
                                $(".video").css("margin-bottom", "20px");
                            }

                            setTimeout(function() {
                                $("#clickbank-disclaimer").slideDown(500);
                            }, 1000);
                        }
                    };
                    clearInterval(searchInterval);
                }
            });
        },1000);
    });
    {% endif %}
</script>
