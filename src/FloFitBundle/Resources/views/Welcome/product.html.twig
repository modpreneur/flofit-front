    {% if useJW7 %}
        <!-- PLAYER -->
        <script src="http://content.jwplatform.com/libraries/cIS7inOF.js"></script>
        <script>
            function loadPlayer() {
                jwplayer("video").setup({
                    stretching: "uniform",
                    aspectratio: "16:9",
                    primary: "html5",
                    width: "100%",
                    file: "{{ videoSource }}",
                    image: "{{ asset("images/video-thumb-ff.jpg") }}",
                    autostart: true,
                    ga: {}
                });
                // set the volume to 100% just in case the user turned it down
                jwplayer().setVolume(100);
            }

            setTimeout(loadPlayer, 1);
        </script>
        <style>
            .jwplayer .jw-controlbar{
                display: none !important;
            }
        </style>
    {% else %}
        <!-- SCRIPTS - Player START -->
        <script src="{{ asset("js/jwplayer6.js") }}"></script>
        <script>
            function loadPlayer() {
                jwplayer("video").setup({
                    stretching: "uniform",
                    primary: "flash",
                    aspectratio: "16:9",
                    width: "100%",
                    file: "{{ videoSource }}",
                    image: "{{ asset("images/video-thumb-ff.jpg") }}",
                    autostart: true,
                    skin: "{{ asset("skin-mobile.xml", "app") }}",
                    ga: {}
                });
                // set the volume to 100% just in case the user turned it down
                jwplayer().setVolume(100);
            }

            setTimeout(loadPlayer, 1);
        </script>
        <!-- SCRIPTS - Player END -->
    {% endif %}
</head>
